unit dim Distance
unit dim Time
unit dim Mass

unit dim Speed = [[Distance / Time]]
unit dim Acceleration = [[Speed / Time]]
unit dim Force = [[Acceleration * Mass]]
unit dim Energy = [[Force * Distance]]
unit dim Torque = [[Force * Distance]]

unit prefix SI = [
    c: 0.1
    k: 1000
]

unit m = [[Distance]]
unit km = 1000[m]

unit J = [[Energy]]
unit eV = 1.602176634e-19[J]

val d = 5.2 [km]
println(d + 2[m] to [m])

d = 10[J]
println(d / 10[m] to [eV/km])